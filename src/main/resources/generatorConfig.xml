<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动地址-->
    <context id="myContext" targetRuntime="MyBatis3Simple">
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 注释配置 -->
        <commentGenerator>
            <!-- 是否去除全部注释 -->
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>
        <!--数据库链接配置-->
        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://192.168.1.73:8888/school"
                userId="china"
                password="chinese"/>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>
        <!-- 生成模型配置 -->
        <javaModelGenerator targetPackage="domains" targetProject="src">
            <!-- 所有字段的构造函数 -->
            <property name="constructorBased" value="true"/>
            <!-- 是否允许子包，即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="false"/>
            <!-- 建立的Model对象是否不可改变,即生成的Model对象不会有 setter方法，只有构造方法 -->
            <property name="immutable" value="false"/>
            <!-- 给Model添加一个父类 -->
            <!-- <property name="rootClass" value="com.kuwei.appserver"/> -->
            <!-- 是否对类CHAR类型的列的数据进行trim操作 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成Mapper配置 -->
        <sqlMapGenerator targetPackage="mapper" targetProject="src">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="mapper" targetProject="src">
            <property name="enableSubPackages" value="false"/>
            <!--为生成的接口添加父接口
            <property name="rootInterface" value=""/> -->
        </javaClientGenerator>
        <!-- 要生成的表 tableName是数据库中的表名或视图名domainObjectName是实体类名-->
        <table schema="moore_core"
               tableName="talent_pool"
               domainObjectName="TalentPool"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <property name="constructorBased" value="true"/>
            <!-- sqlStatement: DB2 Derby HSQLDB MySql SqlServer SYBASE JDBC -->
            <generatedKey column="id" sqlStatement="JDBC" identity="true"/>
        </table>
    </context>
</generatorConfiguration>
